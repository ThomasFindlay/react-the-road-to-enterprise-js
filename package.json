{
  "name": "react-the-road-to-enterprise",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "preview": "npm run build && vite preview --port 4173",
    "test:unit:coverage": "vitest run --coverage --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "npx stylelint \"./**/*.{css,scss,sass}\" --config \"./stylelint.config.cjs\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier:fix": "prettier --config ./prettier.config.cjs --write ./src/**/*.{css,scss,js,jsx} ",
    "prettier:check": "prettier --config ./prettier.config.cjs --check ./src/**/*.{css,scss,js,jsx}",
    "prepare": "husky install",
    "build": "vite build",
    "serve": "vite preview",
    "test:unit": "vitest --environment jsdom"
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": "npm run lint",
    "./src/*.{css,scss,sass}": "npm run stylelint",
    "./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier -w -u"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.5",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "prop-types": "^15.8.1",
    "react-intersection-observer": "^9.4.3",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "4.0.4",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "cypress": "^12.13.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.28",
    "postcss-extend": "^1.0.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-html": "^1.4.1",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.3",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.1.1",
    "postcss-reporter": "^7.0.5",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.10.3",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-scss": "^5.1.0",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "whatwg-fetch": "^3.6.2",
    "@types/node": "^20.5.5",
    "lint-staged": "14.0.1",
    "@testing-library/user-event": "^14.4.3",
    "husky": "^8.0.3",
    "optionator": "^0.9.3"
  },
  "type": "module"
}